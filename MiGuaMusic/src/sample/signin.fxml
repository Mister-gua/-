<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="sample.SignInController"
      spacing="10"
      prefHeight="400.0" prefWidth="600.0">
    <HBox spacing="10.0">
        <Label text="用户名"></Label>
        <TextField fx:id="name"></TextField>
    </HBox>
    <HBox spacing="25.0">
        <Label text="密码"></Label>
        <PasswordField fx:id="password"></PasswordField>
    </HBox>
    <HBox spacing="10.0" style="-fx-padding: 0 0 0 200">
        <Button text="登录" onMouseClicked="#signIn" ></Button>
    </HBox>
    <padding>
        <Insets top="20.0" left="20.0" right="20.0" bottom="20.0"/>
    </padding>
</VBox>
